<div class="page-header">
  <%= link_to checklistitems_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_checklistitem_path(@checklistitem), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show checklistitem</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @checklistitem.title %></dd>

  <dt>Checklist:</dt>
  <dd><%= @checklistitem.checklist %></dd>

</dl>
