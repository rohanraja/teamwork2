<div class="page-header">
  <%= link_to testcases_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_testcase_path(@testcase), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show testcase</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @testcase.title %></dd>

  <dt>Description:</dt>
  <dd><%= @testcase.description %></dd>

</dl>
